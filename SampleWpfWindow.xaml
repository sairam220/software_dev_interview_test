<Window x:Class="RhinoSamplePlugin.SampleWpfWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Sample Rhino Plugin" Height="300" Width="400">
    <Grid>
        <TextBox x:Name="LengthTextBox" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10" />
        <TextBox x:Name="WidthTextBox" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,40,0,0" />
        <TextBox x:Name="XTextBox" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,80,0,0" />
        <TextBox x:Name="YTextBox" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,120,0,0" />
        <TextBox x:Name="ZTextBox" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,160,0,0" />
        <Button Content="Generate" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,200,0,0" Click="GenerateButton_Click" />
    </Grid>
</Window>
